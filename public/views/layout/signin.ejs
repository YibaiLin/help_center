<div class="sign-box">
  <div class="center-form">
    <div class="row">
      <div class="col-sm-5 d-flex justify-content-center align-items-center brand-wrap">
            <img class="brand-img" width="64" height="64" src="/image/edns.png">
            <span class="align-self-end font-weight-bold brand-text">Edns IT</span>
      </div>
      <div class="col-sm-6 form-wrap">
        <form  method="POST" action="/user/signin">
          <div class="form-group">
            <label for="name" class="text-muted">Name</label>
            <input name="name" type="text" class="form-control" placeholder="Enter name">
          </div>
          <div class="form-group">
            <label for="password1" class="text-muted">Password</label>
            <input name="password" type="password" class="form-control" id="password1" placeholder="Password">
          </div>
          <button type="submit" class="btn btn-primary">Sign In</button>
        </form>
      </div>
    </div>
  </div>
  <p class="text-center text-muted">Powered by <b>NodeJS</b></p>
</div>